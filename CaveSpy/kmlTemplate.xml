  <Folder>
    <name>CaveSpy.exe</name>
    <description>Examples of ground overlays</description>
    <GroundOverlay>
      <name>**OverlayName**</name>
      <description>**Description**</description>
      <Icon>
        <href>**Ref**</href>
      </Icon>
      <LatLonBox>
        <north>**North**</north>
